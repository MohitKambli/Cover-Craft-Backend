{
  "version": 2,
  "builds": [
    {
      "src": "app/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app/index.py"
    }
  ],
  "rewrites": [
    {
      "source": "/generate-cover-letter",
      "destination": "https://cover-craft-backend.vercel.app/generate-cover-letter"
    }
  ]
  
}
